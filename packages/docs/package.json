{
  "name": "@kubricate/docs",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/dts/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/dts/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "pre:dev": "typedoc --out api/core --options typedoc.core.json",
    "dev": "vitepress dev",
    "build": "run-s pre:dev docs:build",
    "docs:build": "vitepress build",
    "preview": "vitepress preview"
  },
  "devDependencies": {
    "@kubricate/config-eslint": "workspace:*",
    "@kubricate/config-typescript": "workspace:*",
    "@kubricate/config-vitest": "workspace:*",
    "@kubricate/core": "workspace:*",
    "@kubricate/env": "workspace:*",
    "@kubricate/kubernetes": "workspace:*",
    "@kubricate/mono": "workspace:*",
    "@kubricate/stacks": "workspace:*",
    "@shikijs/vitepress-twoslash": "^1.5.2",
    "@thaitype/vitepress-typed-navbar": "^1.0.1",
    "@types/bun": "latest",
    "@types/node": "^20.12.12",
    "kubernetes-models": "^4.4.2",
    "kubricate": "workspace:*",
    "ts-odata-client": "^2.0.2",
    "vitepress": "^1.2.2",
    "vue": "^3.4.27",
    "typedoc": "^0.28.3",
    "typedoc-plugin-markdown": "^4.6.3",
    "typedoc-vitepress-theme": "^1.1.2"
  }
}